import java.util.*;


class Main {

  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int lines = sc.nextInt();
    for(int i = 0; i < lines; i++) {
      int num = sc.nextInt();
      if(num%2 == 0) {
        System.out.println(num + " is even");
      }
      else {
        System.out.println(num + " is odd");
      }
    }
  }
}
